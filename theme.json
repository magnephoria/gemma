{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"patterns": [
		"three-columns-of-services",
		"clients-section"
	],
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "720px",
			"wideSize": "1400px"
		},
		"color": {
			"defaultPalette": false,
			"defaultGradients": true,
			"defaultDuotone": true,
			"custom": true,
			"gradients": [
				{
					"slug": "gradient-1",
					"gradient": "linear-gradient(45deg, var(--wp--preset--color--accent) 0%, var(--wp--preset--color--accent-2) 100%)",
					"name": "Accent to Accent / Two"
				},
				{
					"slug": "gradient-2",
					"gradient": "linear-gradient(45deg, var(--wp--preset--color--accent) 0%, var(--wp--preset--color--accent-3) 100%)",
					"name": "Accent to Accent / Three"
				},
				{
					"slug": "gradient-3",
					"gradient": "linear-gradient(45deg, var(--wp--preset--color--accent-3) 0%, var(--wp--preset--color--accent-4) 100%)",
					"name": "Accent / Three to Accent / Four"
				},
				{
					"slug": "gradient-4",
					"gradient": "linear-gradient(45deg, var(--wp--preset--color--accent) 0%, var(--wp--preset--color--accent-4) 100%)",
					"name": "Accent to Accent / Four"
				},
				{
					"slug": "gradient-5",
					"gradient": "linear-gradient(45deg, var(--wp--preset--color--accent) 0%, var(--wp--preset--color--accent-5) 100%)",
					"name": "Accent to Accent / Five"
				}
			],
			"palette": [
				{
					"color": "#FFFCF3",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#FFFFFF",
					"name": "Base / Two",
					"slug": "base-2"
				},
				{
					"color": "#14120F",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#524D41",
					"name": "Contrast / Two",
					"slug": "contrast-2"
				},
				{
					"color": "#A79F90",
					"name": "Contrast / Three",
					"slug": "contrast-3"
				},
				{
					"color": "#DADEBA",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#F9D8AD",
					"name": "Accent / Two",
					"slug": "accent-2"
				},
				{
					"color": "#E0CCD5",
					"name": "Accent / Three",
					"slug": "accent-3"
				},
				{
					"color": "#D1CDDF",
					"name": "Accent / Four",
					"slug": "accent-4"
				},
				{
					"color": "#C8DCE4",
					"name": "Accent / Five",
					"slug": "accent-5"
				}
			],
			"duotone": [
				{
					"colors": [
						"#111111", "#E8DEC4"
					],
					"slug": "duotone-1",
					"name": "Black and pastel gold"
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"customSpacingSize": true,
			"spacingSizes": [
				{
					"name": "1",
					"size": "min(1rem, 2.5vw)",
					"slug": "20"
				},
				{
					"name": "2",
					"size": "min(1.5rem, 4vw)",
					"slug": "30"
				},
				{
					"name": "3",
					"size": "min(2.25rem, 5vw)",
					"slug": "40"
				},
				{
					"name": "4",
					"size": "min(4rem, 7vw)",
					"slug": "50"
				},
				{
					"name": "5",
					"size": "min(6.5rem, 10vw)",
					"slug": "60"
				},
				{
					"name": "6",
					"size": "min(10.5rem, 12vw)",
					"slug": "70"
				},
				{
					"name": "7",
					"size": "min(13rem, 17vw)",
					"slug": "80"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": false,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.65rem"
					},
					"size": "2.65rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "3.5rem"
					},
					"size": "3.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.65rem",
						"max": "6rem"
					},
					"size": "6rem",
					"slug": "xx-large"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Uncut Sans\", Arial, Helvetica Neue, Helvetica, sans-serif",
					"name": "Uncut Sans",
					"slug": "uncut-sans",
					"fontFace": [
						{
							"fontFamily": "Uncut Sans",
							"fontStyle": "oblique 0 11deg",
							"fontVariationSettings": "'ital' 0",
							"fontFeatureSettings": "'ss01' 1, 'ss04' 0, 'ss07' 1",
							"fontWeight": "300 700",
							"src": [
								"file:./assets/fonts/uncut-sans/UncutSans-Variable.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "\"Nyght Serif\", TimesNewRoman, \"Times New Roman\", Times, serif",
					"name": "Nyght Serif",
					"slug": "nyght-serif",
					"fontFace": [
						{
							"fontFamily": "Nyght Serif",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/nyght-serif/NyghtSerif-Light.woff2"
							]
						},
						{
							"fontFamily": "Nyght Serif",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/nyght-serif/NyghtSerif-LightItalic.woff2"
							]
						},
						{
							"fontFamily": "Nyght Serif",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/nyght-serif/NyghtSerif-Regular.woff2"
							]
						},
						{
							"fontFamily": "Nyght Serif",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/nyght-serif/NyghtSerif-RegularItalic.woff2"
							]
						},
						{
							"fontFamily": "Nyght Serif",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/nyght-serif/NyghtSerif-Bold.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "\"Newsreader\", TimesNewRoman, \"Times New Roman\", Times, serif",
					"name": "Newsreader",
					"slug": "newsreader",
					"fontFace": [
						{
							"fontFamily": "Newsreader",
							"fontStyle": "normal",
							"fontWeight": "200 800",
							"src": [
								"file:./assets/fonts/newsreader/Newsreader[opsz,wght].woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Arial, FreeSans, \"Helvetica Neue\", Helvetica, sans-serif",
					"name": "Arial",
					"slug": "arial"
				},
				{
					"fontFamily": "TimesNewRoman, \"Times New Roman\", Times, FreeSerif, \"Nimbus Roman D\", Baskerville, Georgia, serif",
					"name": "Times New Roman",
					"slug": "times-new-roman"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"right": "var(--wp--preset--spacing--40)",
				"left": "var(--wp--preset--spacing--40)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
			"fontWeight": "400",
			"lineHeight": "1.6",
			"fontSize": "var(--wp--preset--font-size--medium)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--newsreader)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"h1": {
				"typography": {
					"fontWeight": "300",
					"lineHeight": "1.2",
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontWeight": "350",
					"lineHeight": "1.3",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontWeight": "350",
					"lineHeight": "1.3",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontWeight": "450",
					"lineHeight": "1.5",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontWeight": "500",
					"lineHeight": "1.2",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontWeight": "500",
					"lineHeight": "1.2",
					"textTransform": "uppercase",
					"letterSpacing": "0.04em",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"button": {
				"border": {
					"radius": "99px"
				},
				"spacing": {
					"padding": {
						"top": "0.6rem !important",
						"right": "1.2rem !important",
						"bottom": "0.7rem !important",
						"left": "1.2rem !important"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1",
					"textDecoration": "none"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				":focus": {
					"outline": {
						"style": "auto"
					}
				},
				"css": "& {transform: translateY(0); transition: 0.3s all ease;} &:hover {transform: scale(0.97);} &:active {transform: scale(0.97);}"
			},
			"link": {
				"typography": {
					"textDecoration": "underline"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": { "ref": "styles.elements.heading.typography.fontFamily" },
					"fontWeight": "350",
					"lineHeight": "1.2",
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/navigation": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--40)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"typography": {
					"fontWeight": "400",
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"css": "&-item.current-menu-item > a { text-decoration: underline !important } &-item.current-menu-ancestor > a { text-decoration: underline !important }"
			},
			"core/navigation-submenu": {
				"css": "&.wp-block-navigation__submenu-container { font-size: var(--wp--preset--font-size--small); } &.wp-block-navigation__submenu-container .wp-block-navigation-item a { padding: 0.5em 1.25em; } &.wp-block-navigation__submenu-container .wp-block-navigation-item:first-child a { padding-top: 1em; } &.wp-block-navigation__submenu-container .wp-block-navigation-item:last-child a { padding-bottom: 1em; } &.has-background.wp-block-navigation__submenu-container { border: none !important; }"
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "1.618rem",
					"margin": {
						"top": "1.25em",
						"bottom": "1.25em"
					}
				}
			},
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"width": "1px"
						}
					}
				}
			},
			"core/details": {
				"border": {
					"width": "2px",
					"style": "solid",
					"radius": "2rem"
				},
				"spacing": {
					"padding": {
						"top": "0.7rem",
						"right": "1.5rem",
						"bottom": "0.7rem",
						"left": "1.5rem"
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontFamily": { "ref": "styles.typography.fontFamily" },
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"border": {
					"width": "0 0 0 1px",
					"color": "var(--wp--preset--color--contrast)",
					"style": "solid"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": { "ref": "styles.typography.fontFamily" },
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontWeight": "400",
							"fontStyle": "normal",
							"textTransform": "uppercase",
							"letterSpacing": "0.04em"
						},
						"color": {
							"text": "var(--wp--preset--color--contrast-2)"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)"
					}
				},
				"variations": {
					"plain": {
						"border": {
							"width": "0"
						},
						"spacing": {
							"padding": {
								"left": "var(--wp--preset--spacing--30)",
								"right": "var(--wp--preset--spacing--30)"
							}
						}
					}
				}
			},
			"core/pullquote": {
				"typography": {
					"fontFamily": { "ref": "styles.elements.heading.typography.fontFamily" },
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"border": {
					"style": "solid",
					"width": "1px 0",
					"color": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": { "ref": "styles.typography.fontFamily" },
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontWeight": "400",
							"fontStyle": "normal",
							"textTransform": "uppercase",
							"letterSpacing": "0.04em"
						},
						"color": {
							"text": "var(--wp--preset--color--contrast-2)"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"top": "var(--wp--preset--spacing--30)"
					}					
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"bottom": "1.618rem",
						"top": "1.618rem"
					}
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dotted"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--accent)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-terms": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline dotted"
							}
						},
						"typography": {
							"fontFamily": { "ref": "styles.elements.heading.typography.fontFamily" },
							"fontSize": "var(--wp--preset--font-size--medium)"
						}
					}
				},
				"css": "& .wp-block-post-terms__prefix{color: var(--wp--preset--color--contrast-2);}"
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/avatar": {
				"border": {
					"radius": "99rem"
				}
			},
			"core/post-author": {
				"css": "&__avatar{border-radius: 99rem; overflow: hidden; aspect-ratio: 1/1;}"
			},
			"core/post-author-name": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase",
					"letterSpacing": "0.04em"
				}
			},
			"core/post-date": {
				"color": {
					"text": "var(--wp--preset--color--contrast-2)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast-2)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase",
					"letterSpacing": "0.04em"
				}
			},
			"core/post-navigation-link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/query-pagination-previous": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/query-pagination-next": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/query": {
				"elements": {
					"heading": {
						"typography": {
							"fontSize": { "ref": "styles.elements.h2.typography.fontSize" },
							"fontWeight": { "ref": "styles.elements.h2.typography.fontWeight" }
						},
						"color": {
							"text": { "ref": "styles.elements.h2.color.text" }
						}
					}
				}
			},
			"core/search": {
				"typography": {
					"fontFamily": { "ref": "styles.typography.fontFamily" }
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"style": "solid",
